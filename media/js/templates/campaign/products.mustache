<div class="form-group row">
	<label for="inputKeyword" class="col-sm-2 form-control-label">Searchby keyword</label>
	<div class="col-sm-10">
		<input type="text" class="form-control" id="inputKeyword"
			placeholder="e.g Electric Guitar">
	</div>
</div>
<div id="loading" class="hidden">Loading...</div>

<div id="products-wrapper">

<div id="products" class="row list-group">
{{#products}}
	<div class="item col-xs-4 col-lg-4" data-itemid="{{itemId}}">
		<div class="thumbnail">
			<img class="group list-group-image" src="{{galleryURL}}" alt="" />
			<div class="caption">
<!-- 				<h4 class="group inner list-group-item-heading">{{title}}</h4> -->
				<p class="group inner list-group-item-text">{{title}}123</p>
				<div class="row">
					<div class="col-xs-12 col-md-6">
						<p class="lead">${{sellingStatus.convertedCurrentPrice.amount}}</p>
					</div>
				</div>
			</div>
		</div>
	</div>
{{/products}}
</div>
<div id="selected-products">
</div>

</div>
